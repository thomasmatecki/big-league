    "next": "10.2.3",
    "next-iron-session": "^4.2.0",
    "polished": "^4.1.3",
    "ramda": "^0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.1",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.1",
    "@types/cors": "^2.8.12",
    "@types/ramda": "github:types/npm-ramda#dist",
    "@types/react": "17.0.9",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "typescript": "4.3.2",
